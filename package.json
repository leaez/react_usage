    "axios": "^0.19.0",
    "babel-plugin-import": "^1.12.1",
    "classnames": "^2.2.6",
    "customize-cra": "^0.7.0",
    "electron-is-dev": "^1.0.1",
    "enquire-js": "^0.2.1",
    "formik": "^1.5.8",
    "http-proxy": "^1.17.0",
