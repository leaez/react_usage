    "url": "https://your-website.com"
  },
  "build": {
    "appId": "com.b0tr.app",
    "productName": "b0tr",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "assets"
    }
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
