    "rc-tree": "^2.1.2",
    "react": "^16.9.0",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.0",
    "react-list-select": "^0.4.1",
    "react-redux": "^7.1.1",
    "react-reflex": "^3.0.22",
    "react-scripts": "^4.0.3",
    "react-tabs": "^3.0.0",
    "styled-components": "^5.3.0",
    "three": "^0.125.0"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "startx": "react-scripts start",
    "buildx": "react-scripts build",
    "testx": "react-scripts test",
    "ejectx": "react-scripts eject",
    "startxx": "react-scripts start",
    "buildxx": "react-scripts build",
    "testxx": "react-scripts test",
    "startr": "rescripts start",
    "buildr": "rescripts build",
    "testr": "rescripts test",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "electron-builder -l"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "concurrently": "^5.1.0",
    "electron": "^8.2.3",
    "electron-builder": "^22.4.1",
    "postcss": "^8.3.5",
    "tailwindcss": "^2.2.4",
    "twin.macro": "^2.5.0",
